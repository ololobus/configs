[user]
        name = Alexey Kondratov
        email = alex.lumir@gmail.com
[alias]
        put = push origin
        co = checkout
        st = status
        ftch = fetch -p
	pl = pull --rebase
	br = branch
	viz = log --graph --decorate --oneline --all
[push]
        default = simple
[filter "lfs"]
        clean = git-lfs clean -- %f
        smudge = git-lfs smudge -- %f
	required = true

[core]
	excludesfile = ~/.gitignore
